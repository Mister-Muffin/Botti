{
  "name": "discordbot",
  "version": "1.0.0",
  "description": "Botti with node.js",
  "main": "bot/main.cjs",
  "scripts": {
    "test": "node bot/main.cjs debug",
    "start": "node bot/main.cjs",
    "express": "node backend/dist/server.js",
    "full": "concurrently \"node bot/main.cjs\" \"node backend/dist/server.js\"",
    "register": "node bot/main.cjs --register",
    "delete": "node bot/main.cjs --delete",
    "dev": "vite botti",
    "build": "run-p type-check build-only",
    "preview": "vite botti preview",
    "build-only": "vite build",
    "build-only-dev": "vite build --mode development",
    "build-server": "tsc -p backend/tsconfig.json",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "version": "node -v"
  },
  "author": "",
  "license": "GNU General Public License v3.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "dependencies": {
    "@discordjs/voice": "^0.13.0",
    "ascii-table": "^0.0.9",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^6.11.2",
    "mkdirp": "^1.0.4",
    "pg": "^8.11.4",
    "vue": "^3.4.21",
    "web-pingjs": "^1.0.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.1",
    "@tsconfig/node16": "^1.0.4",
    "@types/express": "^4.17.21",
    "@types/node": "^16.18.93",
    "@types/pg": "^8.11.4",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "typescript": "~4.7.4",
    "vite": "^3.2.10",
    "vue-tsc": "^1.8.27"
  },
  "type": "module"
}